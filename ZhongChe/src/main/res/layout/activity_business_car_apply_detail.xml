<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/WhiteColor"
    android:orientation="vertical">

    <TextView
        android:id="@+id/frg_status_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:visibility="gone"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent">
        <LinearLayout
            android:id="@+id/btn_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:padding="@dimen/horizontal_margin">
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:src="@mipmap/icon_white_back"/>
        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="我的申请"
            android:textColor="@color/title_text"
            android:textSize="@dimen/size16sp"/>
        <TextView
            android:id="@+id/tv_save"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="保存"
            android:textColor="@color/title_text"
            android:textSize="@dimen/size16sp"/>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:id="@+id/ll_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="申请人"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_apply_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="王五"
                    android:textSize="15sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="联系方式"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <EditText
                    android:id="@+id/et_apply_mobile"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:hint="填写联系方式"
                    android:textColorHint="@color/HintTextColor"
                    android:layout_marginLeft="10dp"
                    android:textSize="14sp"
                    android:text="18666860912"
                    android:textColor="@color/WeakTextColor"
                    android:background="@color/WhiteColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:text="核算主体"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_apply_accounting_subject"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"
                    android:layout_marginLeft="10dp"
                    android:text="选择核算主体"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="部门"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_apply_apartment"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text=""
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="申请日期"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_apply_time"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="2018-06-25"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_light_gray">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="5dp"
                    android:layout_centerInParent="true"
                    android:text="用车信息"
                    android:textSize="14sp"
                    android:textColor="#000000"/>
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll_add_car_user"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <include layout="@layout/layout_line"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_add_car_user_add"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/colorAccent"
                        android:text="＋"/>
                    <TextView
                        android:id="@+id/tv_add_car_user_tips"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:textSize="14sp"
                        android:textColor="@color/colorAccent"
                        android:text="添加用车人"/>
                </LinearLayout>

            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:id="@+id/ll_car_user"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:paddingRight="10dp">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginRight="10dp"
                        android:text="用车人姓名"
                        android:textSize="14sp"
                        android:textColor="@color/colorFontBlue"/>
                    <EditText
                        android:id="@+id/et_user_name"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:layout_marginLeft="10dp"
                        android:singleLine="true"
                        android:textSize="14sp"
                        android:textColor="@color/WeakTextColor"
                        android:hint="填写用车人姓名"
                        android:textColorHint="@color/HintTextColor"
                        android:background="@color/WhiteColor"/>
                </LinearLayout>
                <include layout="@layout/layout_line"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:paddingRight="10dp">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginRight="10dp"
                        android:text="用车人电话"
                        android:textSize="14sp"
                        android:textColor="@color/colorFontBlue"/>
                    <EditText
                        android:id="@+id/et_user_mobile"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:layout_marginLeft="10dp"
                        android:singleLine="true"
                        android:inputType="phone"
                        android:textSize="14sp"
                        android:textColor="@color/WeakTextColor"
                        android:hint="填写用车人电话"
                        android:textColorHint="@color/HintTextColor"
                        android:background="@color/WhiteColor"/>
                </LinearLayout>
                <include layout="@layout/layout_line"/>
                <LinearLayout
                    android:id="@+id/ll_is_important_client"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:paddingRight="10dp"
                    android:gravity="center_vertical">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="是否重要客户"
                        android:textSize="16sp"
                        android:textColor="@color/colorFontBlue"/>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2">
                        <ImageView
                            android:id="@+id/iv_is_important_client"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_marginLeft="40dp"
                            android:src="@mipmap/btn_singleselection_nor"/>
                    </RelativeLayout>
                </LinearLayout>
                <include layout="@layout/layout_line"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingTop="8dp"
                    android:paddingBottom="8dp"
                    android:paddingRight="10dp">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="客户职务和级别"
                        android:textSize="14sp"
                        android:textColor="@color/colorFontBlue"/>
                    <EditText
                        android:id="@+id/et_client_level"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:layout_marginLeft="10dp"
                        android:singleLine="true"
                        android:maxLength="50"
                        android:textSize="14sp"
                        android:textColor="@color/WeakTextColor"
                        android:hint="填写客户职务和级别"
                        android:textColorHint="@color/HintTextColor"
                        android:background="@color/WhiteColor"/>
                </LinearLayout>
                <include layout="@layout/layout_line"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="用车类型"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <LinearLayout
                    android:id="@+id/ll_car_type"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_car_type"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:text="选择用车类型"
                        android:textSize="14sp"
                        android:textColor="@color/WeakTextColor"/>
                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="10dp"
                        android:src="@mipmap/icon_arrow_down_gray"/>
                </LinearLayout>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="用车数量"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <EditText
                    android:id="@+id/et_car_number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:singleLine="true"
                    android:inputType="number"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"
                    android:hint="填写用车数量"
                    android:textColorHint="@color/HintTextColor"
                    android:background="@color/WhiteColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="乘车人数"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <EditText
                    android:id="@+id/et_people_number"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:inputType="number"
                    android:text="2"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"
                    android:background="@color/WhiteColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="用车开始时间"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_start_time"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="2018-05-16"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="用车结束时间"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_end_time"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="2018-6-26"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="用车范围"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <LinearLayout
                    android:id="@+id/ll_car_range"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_car_range"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="10dp"
                        android:text="选择用车范围"
                        android:textSize="14sp"
                        android:textColor="@color/WeakTextColor"/>
                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="10dp"
                        android:src="@mipmap/icon_arrow_down_gray"/>
                </LinearLayout>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:gravity="center_vertical"
                    android:text="申请事由"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <EditText
                    android:id="@+id/et_apply_reason"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="2"
                    android:background="@color/WhiteColor"
                    android:gravity="top"
                    android:hint="填写申请事由"
                    android:text="事由"
                    android:maxLines="4"
                    android:textColor="@color/WeakTextColor"
                    android:textColorHint="@color/HintTextColor"
                    android:textSize="14sp" />
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="审批状态"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_apply_status"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="待审批"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_light_gray">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="8dp"
                    android:text="审批信息"
                    android:textSize="14sp"
                    android:textColor="#000000"/>
            </RelativeLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="部门审批意见"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_department_advice"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="意见"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="总经办审批意见"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_officer_manager_advice"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="意见"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
            <include layout="@layout/layout_line"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="8dp"
                android:paddingBottom="8dp"
                android:paddingRight="10dp"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp"
                    android:text="办理意见"
                    android:textSize="14sp"
                    android:textColor="@color/NormalTextColor"/>
                <TextView
                    android:id="@+id/tv_handler_advice"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginLeft="10dp"
                    android:text="意见"
                    android:textSize="14sp"
                    android:textColor="@color/WeakTextColor"/>
            </LinearLayout>
        </LinearLayout>

    </ScrollView>

</LinearLayout>

