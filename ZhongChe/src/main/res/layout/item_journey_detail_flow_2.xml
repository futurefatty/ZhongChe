<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:sy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/tv_place_holde_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="19:40"
        android:textColor="@color/C333333"
        android:textSize="@dimen/common_textsize_12"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.crcc.commonlib.view.DashView
        android:layout_width="@dimen/D2"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/D23_5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/tv_place_holde_1"
        app:layout_constraintTop_toTopOf="parent"
        sy:dashOrientation="1"
        sy:dashWidth="@dimen/D2"
        sy:lineColor="@color/CE2E2E2"
        sy:lineHeight="@dimen/D2"
        sy:lineWidth="@dimen/D2" />

    <include
        android:id="@+id/ll_transfer"
        layout="@layout/layout_flight_transfer"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/D19_5"
        android:layout_marginTop="@dimen/D21"
        android:layout_marginBottom="@dimen/D21"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/tv_place_holde_1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>